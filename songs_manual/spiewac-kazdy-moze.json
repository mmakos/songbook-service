    "verified": false
  },
  "verses": [
    {
      "lines": [
        {
          "text": [
            {
              "text": "Śpiewać każdy może, trochę lepiej, lub trochę gorzej,"
            },
            {
              "text": "(w c-moll)",
              "italic": true
            }
          ]
        },
        {
          "text": [
            {
              "text": "Ale nie oto chodzi, jak co komu wychodzi."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Czasami człowiek musi, inaczej się udusi."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Ooo"
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Lubię piosenki, różne inne dźwięki"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Szczególnie jak mnie co wzruszy rzuca mnie się na uszy."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Teraz będę już szczery wybrałem drogę kariery,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Kariery na estradzie, na pewno sobie poradzę."
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Stoję przy mikrofonie, niech mnie który przegoni,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Różne sceny, brygady, już nie dadzą mi rady"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Bo ja się wcale nie chwalę ja po prostu niestety mam talent."
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Jak człowiek wierzy w_siebie, to cała reszta to betka,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Nie ma takiej rury na świecie, której nie można odetkać"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Wejść na estradę i zostać, cała reszta to rzecz prosta."
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "A ja wiem co jest grane, dlatego tutaj zostanę,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Będę śpiewał piosenki, będą klaskać panienki,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Będą dawać mi kwiaty, będę teraz bogaty,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Bo ja się wcale nie chwalę, ja niestety mam talent."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Jak głos wydaje z siebie wszyscy są w siódmym niebie."
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Stoję przy mikrofonie, niech mnie który przegoni,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Wszystkie sceny, brygady, już nie dadzą mi rady"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Nikt mnie tutaj nie kiwnie bo odczuje to dziwnie"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Ja nikogo nie straszę, ja talentem niestety go gaszę."
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Bardzo lubię piosenki, i różne inne dźwięki"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Szczególnie jak mnie co wzruszy rzuca mnie się na uszy"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Ja znam ładne szlagiery pójdę drogą kariery."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Mecyję w ",
              "italic": true
            },
            {
              "text": "……………"
            },
            {
              "text": " zaśpiewałem.",
              "italic": true
            }
          ]
        }
      ],
      "indent": 0
    }
  ],
