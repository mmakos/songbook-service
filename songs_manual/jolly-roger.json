      "lines": [
        {
          "text": [
            {
              "text": "Śmiejesz się do mnie draniu, gdy brew wznoszę swą do góry"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "D"
                      },
                      "minor": true
