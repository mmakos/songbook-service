          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Właśniem przeczytał o stracie Smoleńska,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Ale gdzie Smoleńsk, gdzie Kraków."
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A",
                        "accidental": "FLAT"
