          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A",
                        "accidental": "FLAT"
                      },
                      "minor": true
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Nie jestem dobrym błaznem na te czasy,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Lecz wśród jedwabnych i złotych kutasów"
