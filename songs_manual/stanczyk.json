          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Na balu dobrze ich goszczą."
            }
