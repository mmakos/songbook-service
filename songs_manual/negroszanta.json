          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  }
                ]
              }
            ]
          }
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Do wieczora spływasz ciężkim potem,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
