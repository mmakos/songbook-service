          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Żeby pełno, równo było, eh! w ładowniach."
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  }
                ]
              }
            ]
          }
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Eh! W ładowniach statku przyszła pora,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Będziesz siedział tutaj bracie do wieczora."
            }
