  "title": "Bogurodzica",
  "category": 3,
  "verses": [
    {
      "lines": [
        {
          "text": [
            {
              "text": "Bogurodzica dziewica, Bogiem sławiena Maryja,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "U twego syna Gospodzina matko zwolena, Maryja!"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Zyszczy nam, spuści nam."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Kyrieleison."
            }
          ]
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Twego dziela Krzciciela, bożycze,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Usłysz głosy, napełń myśli człowiecze."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Słysz modlitwe, jąż nosimy,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Oddać raczy, jegoż prosimy:"
            }
          ]
        },
        {
          "text": [
            {
              "text": "A na świecie zbożny pobyt,"
            }
          ]
        },
        {
          "text": [
            {
              "text": "Po żywocie rajski przebyt."
            }
          ]
        },
        {
          "text": [
            {
              "text": "Kyrieleison."
            }
          ]
        }
      ],
      "indent": 0
    }
  ],
