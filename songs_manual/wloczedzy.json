    {
      "lines": [
        {
          "text": [
            {
              "text": "Śpiewamy piosenki o drodze i pracy,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "O braku pieniędzy i braku miłości,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Podnoszą się ze snu miejscowi pijacy,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Słuchają oczami rozumnej przeszłości,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Ktoś wstanie, podejdzie, zapyta – kto my?"
