  "title": "Włóczędzy",
  "category": 1,
  "verses": [
    {
      "lines": [
        {
          "text": [
            {
              "text": "Wchodzimy z twarzami zdrowymi od mrozu,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Siadamy przy ogniu tańczącym z radości,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Ścieramy z nadgarstków odciski powrozów,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
