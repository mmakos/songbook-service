    {
      "lines": [
        {
          "text": [
            {
              "text": "Żegnaj Nowa Szkocjo, niech fale biją w brzeg,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "H"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Tajemnicze twe góry wznoszą szczyty swe."
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Kiedy będę daleko, hen na oceanie złym,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "D"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Czy was kiedyś jeszcze ujrzę, czy zostaną mi sny?"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      },
                      "minor": true
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "D"
                      }
                    }
                  }
                ]
              }
            ]
          }
        }
      ],
      "indent": 1
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Słońce już zachodzi, a ptaki skryły się,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "H"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "W liściach drzew znalazły schronienie swe."
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
