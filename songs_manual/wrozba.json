  "verses": [
    {
      "lines": [
        {
          "text": [
            {
              "text": "Kary nie będzie dla przeciętnych drani,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "A lud ofiary złoży nadaremnie."
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "C"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Morderców będą grzebać z honorami,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true,
                      "additionals": [
