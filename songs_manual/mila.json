      "lines": [
        {
          "text": [
            {
              "text": "Szczur kończy gulasz mdły, już pora wyjść z kantyny,"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Karcianej zapis gry na liście od dziewczyny."
            }
