  "verses": [
    {
      "lines": [
        {
          "text": [
            {
              "text": "Brzęk muchy w pustym dzbanie, co stoi na półce"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Smuga w oczach po znikłej za oknem jaskółce"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Cień ręki na murawie, a wszystko niczyje"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Ledwo się zazieleni, już ufa że żyje"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      },
                      "additionals": [
                        {
                          "elements": [
                            {
                              "interval": 7
                            }
                          ]
                        }
                      ]
                    }
                  }
                ]
              }
            ]
          }
        }
      ],
      "indent": 0
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "A jak dumnie się modrzy u ciszy podnóża"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Jak buńczucznie do boju z mgłą się napurpurza"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "A jest go tak niewiele, że mniej niż niebiesko"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Nic prócz tła, biały obłok z liliową przekreską"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      },
                      "additionals": [
                        {
                          "elements": [
                            {
                              "interval": 7
                            }
                          ]
                        }
                      ]
                    }
                  }
                ]
              }
            ]
          }
        }
      ],
      "indent": 1
    },
    {
      "lines": [
        {
          "text": [
            {
              "text": "Dal świata w ślepiach wróbla spotkanie traw z ciałem"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "A"
                      },
                      "minor": true
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Szmery w studni, ja w lesie, byłeś mgłą - bywałem"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "G"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Usta twoje w alei, świt pod groblą, w młynie"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "F"
                      }
                    }
                  }
                ]
              }
            ]
          }
        },
        {
          "text": [
            {
              "text": "Słońce w bramie na oścież, zgon pszczół w koniczynie"
            }
          ],
          "chords": {
            "chords": [
              {
                "chords": [
                  {
                    "chord": {
                      "note": {
                        "base": "E"
                      }
                    }
                  },
                  {
                    "chord": {
                      "note": {
